{
  "name": "explicacao-testes",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "test": "jest"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "fs": "^0.0.1-security",
    "http": "^0.0.1-security",
    "node-forge": "^1.2.1",
    "ts-jest": "27.1.4"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/jest": "27.4.1",
    "@types/node": "*",
    "@types/supertest": "^2.0.11",
    "babel-jest": "^27.5.1",
    "jest": "27.5.1",
    "request": "^2.88.2",
    "supertest": "^6.2.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.2"
  },
  "jest": {
    "clearMocks": true,
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coverageProvider": "v8",
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[tj]s?(x)"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!variables/.*)"
    ],
    "roots": [
      "tests"
    ]
  }
}
